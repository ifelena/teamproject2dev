<div> My Wish Lsit</div>
<div id = "mywishlist">
  {{#each vlist}}
            <div class = "row">
                    <div class = "col-lg-1">
                         <p>{{this.id}}</p>
                    </div>
                    <div class = "col-lg-3">
                        <p>{{this.product_name}}</p>
                    </div>
                    <div class = "col-lg-5">
                        <p>{{this.product_desc}}</p>
                    </div>
                    <div class = "col-lg-3">
                        <button data-id ="{{this.id}}" class ="updbtn btn btn-info">Delete Item</button>
                    </div>
                    <div class = "col-lg-3">
                        <button data-id ="{{this.id}}" class ="delbtn btn btn-danger">Delete Item</button>
                    </div>
            </div>
        {{/each}}

</div>
<div class = "container">
              <h4>Add Wish List Details</h4>
              <div class = "row">
                    <form class = "create-form">
                           <div class = "form-group">
                                 <label>Product Name :</label>
                                 <input id = "htprodname" type = "text" name = "htsubname">
                                 <label>Enter Description :</label>
                                  <input id = "htproddesc" type = "text" name = "hthwdesc">
                                  <label>Enter Price:</label>
                                   <input id = "htprodprice" type = "text" name = "hthwdesc">
                                 <button id ="prodaddbtn" class = "btn btn-success" type = "submit">Add Homework Details</button>
                          </div>
                    </form>
              </div>
        </div>
